    // #includeEtc "caseDicts/setConstraintTypes"

    WALL2
    {
      type kqRWallFunction;
      value uniform 0.01;
    }

    WALL1
    {
      type kqRWallFunction;
      value uniform 0.01;
    }

    TOP
    {
      type inletOutlet;
      inletValue uniform 0.01;
      value uniform 0.01;
    }

    WALL3
    {
      type kqRWallFunction;
      value uniform 0.01;
    }

    WALL4
    {
      type kqRWallFunction;
      value uniform 0.01;
    }
    BTM
    {
      type kqRWallFunction;
      value uniform 0.01;

    }
