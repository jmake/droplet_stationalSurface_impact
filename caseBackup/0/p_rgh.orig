FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.01e05;

boundaryField
{
    //- Set patchGroups for constraint patches
    // #includeEtc "caseDicts/setConstraintTypes"

    WALL2
    {
        type fixedFluxPressure;
        value uniform 0;   
    }

    WALL1
    {
        type fixedFluxPressure;
        value uniform 0;   
    }

    TOP
    {
         type totalPressure;
         p0 uniform 0;
    }

    WALL3
    {
        type fixedFluxPressure;
        value uniform 0;   
    }
    WALL4
    {
        type fixedFluxPressure;
        value uniform 0;   
    }
    BTM
    {
        type fixedFluxPressure;
        value uniform 0;    
    }
}

